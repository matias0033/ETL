              <properties>
                <property
                  dataType="System.String"
                  description="Especifica la instrucción SQL utilizada por el componente para extraer datos."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    v.venta_id AS ID_Venta,
    dv.producto_id AS ID_Producto,
    v.cliente_id AS ID_Cliente,
    v.vendedor_id AS ID_Empleado,
    v.sucursal_id AS ID_Sucursal,
    CAST(CONVERT(VARCHAR(8), v.fecha, 112) AS INT) AS ID_Periodo,
    dv.cantidad AS Cantidad,
    dv.precio_unitario AS Precio_venta,
    dv.descuento AS Descuento,
    CAST(ROUND((dv.cantidad * dv.precio_unitario) - ((dv.cantidad * dv.precio_unitario) * (dv.descuento / 100)), 0) AS INT) AS Monto_Total
FROM 
    ventas v
JOIN 
    detalle_ventas dv ON v.venta_id = dv.venta_id;</property>
